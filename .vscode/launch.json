  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to remote",
      "address": "localhost",
      "port": 9229
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "73263",
      "skipFiles": ["<node_internals>/**"]
    },

    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Py: Cur File - data_import",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}/data_import",
      "program": "${file}",
      "console": "internalConsole"
      // "justMyCode": false
    },
    {
      "name": "Py: Current File",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${file}",
      "console": "internalConsole",
      "justMyCode": false
    },
    {
      "name": "Py: Backend",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "module": "citation_galaxy",
      "console": "internalConsole"
    }
  ]
}
